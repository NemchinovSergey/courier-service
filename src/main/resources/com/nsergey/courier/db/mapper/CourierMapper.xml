<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nsergey.courier.db.mapper.CourierMapper">

    <select id="findById" resultType="com.nsergey.courier.db.model.Courier">
        SELECT *
        FROM couriers
        WHERE id = #{id}
    </select>

</mapper>